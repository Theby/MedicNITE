<%- model_class = PacienteTipo -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:idTipoPaciente) %>:</strong></dt>
  <dd><%= @paciente_tipo.idTipoPaciente %></dd>
  <dt><strong><%= model_class.human_attribute_name(:tipoPaciente) %>:</strong></dt>
  <dd><%= @paciente_tipo.tipoPaciente %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              paciente_tipos_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_paciente_tipo_path(@paciente_tipo), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              paciente_tipo_path(@paciente_tipo),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
