<%= form_for @paciente_tipo, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :idTipoPaciente, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :idTipoPaciente, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :tipoPaciente, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :tipoPaciente, :class => 'text_field' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                paciente_tipos_path, :class => 'btn' %>
  </div>
<% end %>
