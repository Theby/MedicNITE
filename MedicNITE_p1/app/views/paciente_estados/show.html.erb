<%- model_class = PacienteEstado -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:idEstadoPaciente) %>:</strong></dt>
  <dd><%= @paciente_estado.idEstadoPaciente %></dd>
  <dt><strong><%= model_class.human_attribute_name(:estadoPaciente) %>:</strong></dt>
  <dd><%= @paciente_estado.estadoPaciente %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              paciente_estados_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_paciente_estado_path(@paciente_estado), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              paciente_estado_path(@paciente_estado),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
