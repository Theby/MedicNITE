<%- model_class = AsignacionTipo -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:idTipoAsignacion) %></th>
      <th><%= model_class.human_attribute_name(:tipoAsignacion) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @asignacion_tipos.each do |asignacion_tipo| %>
      <tr>
        <td><%= link_to asignacion_tipo.id, asignacion_tipo_path(asignacion_tipo) %></td>
        <td><%= asignacion_tipo.idTipoAsignacion %></td>
        <td><%= asignacion_tipo.tipoAsignacion %></td>
        <td><%=l asignacion_tipo.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_asignacion_tipo_path(asignacion_tipo), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      asignacion_tipo_path(asignacion_tipo),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_asignacion_tipo_path,
            :class => 'btn btn-primary' %>
