<%- model_class = AsignacionTipo -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:idTipoAsignacion) %>:</strong></dt>
  <dd><%= @asignacion_tipo.idTipoAsignacion %></dd>
  <dt><strong><%= model_class.human_attribute_name(:tipoAsignacion) %>:</strong></dt>
  <dd><%= @asignacion_tipo.tipoAsignacion %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              asignacion_tipos_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_asignacion_tipo_path(@asignacion_tipo), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              asignacion_tipo_path(@asignacion_tipo),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
