<%= form_for @personal_rubro_tipo, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :idTipoPersonalRubro, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :idTipoPersonalRubro, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :tipoPersonalRubro, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :tipoPersonalRubro, :class => 'text_field' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                personal_rubro_tipos_path, :class => 'btn' %>
  </div>
<% end %>
