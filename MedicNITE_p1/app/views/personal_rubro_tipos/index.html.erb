<%- model_class = PersonalRubroTipo -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:idTipoPersonalRubro) %></th>
      <th><%= model_class.human_attribute_name(:tipoPersonalRubro) %></th>
      <!--<th><%#= model_class.human_attribute_name(:created_at) %></th>-->
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @personal_rubro_tipos.each do |personal_rubro_tipo| %>
      <tr>
        <td><%= link_to personal_rubro_tipo.id, personal_rubro_tipo_path(personal_rubro_tipo) %></td>
        <td><%= personal_rubro_tipo.idTipoPersonalRubro %></td>
        <td><%= personal_rubro_tipo.tipoPersonalRubro %></td>
        <!--<td><%#=l personal_rubro_tipo.created_at %></td>-->
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_personal_rubro_tipo_path(personal_rubro_tipo), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      personal_rubro_tipo_path(personal_rubro_tipo),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_personal_rubro_tipo_path,
            :class => 'btn btn-primary' %>
