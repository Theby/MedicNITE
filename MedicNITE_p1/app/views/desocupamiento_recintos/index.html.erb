<%- model_class = DesocupamientoRecinto -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:idDesocupamientoRecinto) %></th>
      <th><%= model_class.human_attribute_name(:idAsignacion) %></th>
      <th><%= model_class.human_attribute_name(:fechaDesocupamiento) %></th>
      <th><%= model_class.human_attribute_name(:horaDesocupamiento) %></th>
      <!--<th><%#= model_class.human_attribute_name(:created_at) %></th>-->
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @desocupamiento_recintos.each do |desocupamiento_recinto| %>
      <tr>
        <td><%= link_to desocupamiento_recinto.id, desocupamiento_recinto_path(desocupamiento_recinto) %></td>
        <td><%= desocupamiento_recinto.idDesocupamientoRecinto %></td>
        <td><%= desocupamiento_recinto.idAsignacion %></td>
        <td><%= desocupamiento_recinto.fechaDesocupamiento %></td>
        <td><%= desocupamiento_recinto.horaDesocupamiento %></td>
        <!--<td><%#=l desocupamiento_recinto.created_at %></td>-->
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_desocupamiento_recinto_path(desocupamiento_recinto), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      desocupamiento_recinto_path(desocupamiento_recinto),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_desocupamiento_recinto_path,
            :class => 'btn btn-primary' %>
