<h1>Listing personas</h1>

<table>
  <thead>
    <tr>
      <th>Nombrespersona</th>
      <th>Apellidospersona</th>
      <th>Fechanacimientopersona</th>
      <th>Rutpersona</th>
      <th>Domiciliopersona</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @personas.each do |persona| %>
      <tr>
        <td><%= persona.nombresPersona %></td>
        <td><%= persona.apellidosPersona %></td>
        <td><%= persona.fechaNacimientoPersona %></td>
        <td><%= persona.rutPersona %></td>
        <td><%= persona.domicilioPersona %></td>
        <td><%= link_to 'Show', persona %></td>
        <td><%= link_to 'Edit', edit_persona_path(persona) %></td>
        <td><%= link_to 'Destroy', persona, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Persona', new_persona_path %>
