<%- model_class = Asignacione -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:idAsignacion) %>:</strong></dt>
  <dd><%= @asignacione.idAsignacion %></dd>
  <dt><strong><%= model_class.human_attribute_name(:perIdPersona) %>:</strong></dt>
  <dd><%= @asignacione.perIdPersona %></dd>
  <dt><strong><%= model_class.human_attribute_name(:pacIdPersona) %>:</strong></dt>
  <dd><%= @asignacione.pacIdPersona %></dd>
  <dt><strong><%= model_class.human_attribute_name(:idPersona) %>:</strong></dt>
  <dd><%= @asignacione.idPersona %></dd>
  <dt><strong><%= model_class.human_attribute_name(:idSala) %>:</strong></dt>
  <dd><%= @asignacione.idSala %></dd>
  <dt><strong><%= model_class.human_attribute_name(:idTipoAsignacion) %>:</strong></dt>
  <dd><%= @asignacione.idTipoAsignacion %></dd>
  <dt><strong><%= model_class.human_attribute_name(:idCama) %>:</strong></dt>
  <dd><%= @asignacione.idCama %></dd>
  <dt><strong><%= model_class.human_attribute_name(:idArea) %>:</strong></dt>
  <dd><%= @asignacione.idArea %></dd>
  <dt><strong><%= model_class.human_attribute_name(:iniFechaAsignacion) %>:</strong></dt>
  <dd><%= @asignacione.iniFechaAsignacion %></dd>
  <dt><strong><%= model_class.human_attribute_name(:iniHoraAsignacion) %>:</strong></dt>
  <dd><%= @asignacione.iniHoraAsignacion %></dd>
  <dt><strong><%= model_class.human_attribute_name(:finFechaAsignacion) %>:</strong></dt>
  <dd><%= @asignacione.finFechaAsignacion %></dd>
  <dt><strong><%= model_class.human_attribute_name(:finHoraAsignacion) %>:</strong></dt>
  <dd><%= @asignacione.finHoraAsignacion %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              asignaciones_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_asignacione_path(@asignacione), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              asignacione_path(@asignacione),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
